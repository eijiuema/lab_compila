class Store

    var Int n

    func get -> Int {
        return self.n;
    }

    func set: Int n {
        self.n = n;
    }

end

class Program

    func run {
        var Store s;
        var Int a;
        s = Store.new;
        a = In.readInt;
        s.set: a;
        Out.println: s.get;
    }

end
