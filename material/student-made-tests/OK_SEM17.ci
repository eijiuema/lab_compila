@annot("check", "assert")
@nce
class Program 
   
    func run {
        
        var Int num = 10;
        
        if num < 15 { 
            Out.println: "pequeno"; 
            assert num < 15, "num < 15";
        } 
        else {
            Out.println: "grande";
            assert num >= 15, "num >= 15";
        }
         
   }   

end
